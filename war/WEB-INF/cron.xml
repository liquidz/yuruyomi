<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
	<cron>
		<url>/admin/cron/twitter</url>
		<description>collect tweets every 15 minutes</description>
		<schedule>every 15 minutes</schedule>
		<timezone>Asia/Tokyo</timezone>
	</cron>

	<cron>
		<url>/admin/cron/user</url>
		<description>collect user data every day</description>
		<schedule>every 24 hours</schedule>
		<timezone>Asia/Tokyo</timezone>
	</cron>

	<cron> 
		<url>/_ah/sessioncleanup?clear</url> 
		<description>Clean up sessions</description> 
		<schedule>every 24 hours</schedule> 
		<timezone>Asia/Tokyo</timezone>
	</cron>

</cronentries>
