<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app 
	xmlns="http://appengine.google.com/ns/1.0">
	<application>yuruyomi</application>
	<version>1</version>
	<static-files>
		<include path="/public_html/**.(ico|css|js|gif|jpg|png|html?)" />
	</static-files>
	<resource-files />

	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
</appengine-web-app>
